{
    "name": "peekabooauth/peekaboo-bundle",
    "description": "Peekaboo Bundle",
    "keywords": ["api", "peekabooauth", "peekaboo", "auth", "oauth", "symfony", "bundle"],
    "type": "symfony-bundle",
    "license": "GPL-3.0-only",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-json": "*",
        "psr/log": "^3.0",
        "symfony/framework-bundle": "^7.4|^8.0",
        "symfony/http-client": "^7.4|^8.0",
        "symfony/http-foundation": "^7.4|^8.0",
        "symfony/http-kernel": "^7.4|^8.0",
        "symfony/routing": "^7.4|^8.0",
        "symfony/security-core": "^7.4|^8.0",
        "symfony/security-http": "^7.4|^8.0",
        "symfony/validator": "^7.4|^8.0"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "8.4"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Peekabooauth\\PeekabooBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Peekabooauth\\PeekabooBundle\\Tests\\": "tests/"
        }
    }
}
