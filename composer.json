{
    "name": "peekabooauth/peekaboo-bundle",
    "description": "Peekaboo Bundle",
    "type": "symfony-bundle",
    "license": "GPL-3.0-only",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Peekabooauth\\PeekabooBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.4",
        "symfony/framework-bundle": "^6.1",
        "symfony/http-foundation": "^6.1",
        "symfony/http-kernel": "^6.1",
        "symfony/routing": "^6.1",
        "symfony/security-core": "^6.1",
        "symfony/security-http": "^6.1",
        "symfony/validator": "^6.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
