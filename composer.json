{
    "name": "peekabooauth/peekaboo-bundle",
    "description": "Peekaboo Bundle",
    "keywords": ["api", "peekabooauth", "peekaboo", "auth", "oauth", "symfony", "bundle"],
    "type": "symfony-bundle",
    "license": "GPL-3.0-only",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "psr/log": "^1|^2|^3",
        "symfony/framework-bundle": "^6.2|^7.0",
        "symfony/http-client": "^6.2|^7.0",
        "symfony/http-foundation": "^6.2|^7.0",
        "symfony/http-kernel": "^6.2|^7.0",
        "symfony/routing": "^6.2|^7.0",
        "symfony/security-core": "^6.2|^7.0",
        "symfony/security-http": "^6.2|^7.0",
        "symfony/validator": "^6.2|^7.0"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^10.5",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Peekabooauth\\PeekabooBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Peekabooauth\\PeekabooBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
